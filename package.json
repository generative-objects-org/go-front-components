{
    "name": "go-front-components",
    "description": "VueJS components used in GO generated apps",
    "authors": "GenerativeObjects",
    "version": "0.2.0",
    "private": true,
    "main": "dist/goFrontComponents.umd.min.js",
    "devDependencies": {
        "@vue/cli-service": "^3.6.0",
        "@vue/test-utils": "1.0.0-beta.20",
        "babel-core": "^6.26.0",
        "babel-jest": "^23.4.0",
        "babel-loader": "^7.1.2",
        "babel-plugin-istanbul": "^4.1.5",
        "babel-plugin-module-resolver": "^3.1.1",
        "babel-preset-env": "^1.6.0",
        "cross-env": "^5.2.0",
        "jest": "^23.4.1",
        "jest-serializer-html": "^5.0.0",
        "jest-vue-preprocessor": "^1.4.0",
        "peer-deps-externals-webpack-plugin": "^1.0.4",
        "regenerator-runtime": "^0.12.0",
        "vue-server-renderer": "^2.6.10",
        "vue-template-compiler": "^2.6.10",
        "vue-template-es2015-compiler": "^1.9.1"
    },
    "peerDependencies": {
        "go-front-libs": "git+https://git.generativeobjects.com/go-source/go-front-libs.git",
        "vue": "^2.6.10",
        "vuetify": "^1.5.12"
    },
    "scripts": {
        "test": "cross-env NODE_ENV=test jest -i --verbose --silent",
        "build": "vue-cli-service build --target lib --name GOFrontComponents src/index.js"
    },
    "jest": {
        "verbose": false,
        "roots": [
            "<rootDir>/test/unit"
        ],
        "moduleFileExtensions": [
            "js"
        ],
        "moduleDirectories": [
            "node_modules"
        ],
        "snapshotSerializers": [
            "jest-serializer-html"
        ],
        "moduleNameMapper": {
            "^@/test$": "<rootDir>/test/index.js",
            "^@/test/(.*)$": "<rootDir>/test/$1",
            "^@/(.*)$": "<rootDir>/src/$1"
        },
        "transform": {
            ".*\\.(j|t)s$": "babel-jest",
            ".*\\.(vue)$": "jest-vue-preprocessor"
        }
    },
    "dependencies": {
        "vue": "^2.6.10",
        "vuetify": "^1.5.13"
    }
}